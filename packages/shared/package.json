{
  "name": "@tireoff/shared",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./validators": {
      "types": "./src/validators/index.ts",
      "default": "./src/validators/index.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "default": "./src/utils/index.ts"
    },
    "./i18n/*": {
      "types": "./src/i18n/*.ts",
      "default": "./src/i18n/*.ts"
    },
    "./messages/*": {
      "default": "./src/i18n/messages/*.json"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  }
}
